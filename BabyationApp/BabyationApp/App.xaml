                                <ColumnDefinition Width=".17*"/>
                                <ColumnDefinition Width=".83*"/>
                            </Grid.ColumnDefinitions>
                    
                            <Grid Grid.Column="0" Padding="0,2" HeightRequest="40" HorizontalOptions="Center" VerticalOptions="Start" >
