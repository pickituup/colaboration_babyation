            <Style x:Key="Form_ElementErrorMessage" BasedOn="{StaticResource Form_BaseLabel}" TargetType="Label">
                <Setter Property="LineBreakMode" Value="NoWrap" />
                <Setter Property="TextColor" Value="{StaticResource Red}" />
                <Setter Property="FontFamily" Value="{StaticResource fontHurmeBold}" />
                <Setter Property="FontSize" Value="16" />
                <Setter Property="FontAttributes" Value="Bold" />
            </Style>
            <!-- END Form entry style -->
            
            <!-- Navigation header style: label -->
            <Style x:Key="GlobalHeaderLabel" TargetType="localViews:LabelEx">
                <Setter Property="BackgroundColor" Value="{StaticResource Clear}" />
                <Setter Property="LineBreakMode" Value="NoWrap" />
