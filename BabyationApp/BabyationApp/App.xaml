            <Style x:Key="List_ColumnHeader" BasedOn="{StaticResource Form_ElementHeader}" TargetType="Label">
                <Setter Property="TextColor" Value="{StaticResource Gold}" />
                <Setter Property="FontFamily" Value="{StaticResource fontHurmeSemiBold}" />
                <Setter Property="FontSize" Value="14" />
            </Style>
            <Style x:Key="List_DescriptionSubText" TargetType="Label">
                <Setter Property="TextColor" Value="{StaticResource Navy}" />
                <Setter Property="FontFamily" Value="{StaticResource fontHurmeSemiBold}" />
                <Setter Property="FontSize" Value="10" />
            </Style>
            
            <Style x:Key="TimerLabelModified" BasedOn="{StaticResource Timer_Label}" TargetType="Label">
                <Setter Property="TextColor" Value="{StaticResource MedBlue}" />
                <Setter Property="Margin">
                    <Setter.Value>
                        <OnPlatform x:TypeArguments="Thickness" iOS="0,0,0,0" Android="0,-2,0,0" />
                    </Setter.Value>
                </Setter>
            </Style>
            
            <!-- Listview and Cell styles and templates -->
            <!-- Experience -->
            <!-- Datamodel: ModeItem -->
            <!-- Note: SelectModeCommand should be set -->
            <!--  -->
            
            
            
            <!-- Selectable mode cell -->
            <DataTemplate x:Key="ModeSelectableCellTemplate">
                <ViewCell>
                    <Grid Padding="2,3" MinimumHeightRequest="70" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" Margin="0,10,0,10">
                        <Grid ColumnSpacing="0" RowSpacing="0" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width=".17*"/>
                                <ColumnDefinition Width=".83*"/>
                            </Grid.ColumnDefinitions>
                    
                            <Grid Grid.Column="0" Padding="0,2" HeightRequest="40" HorizontalOptions="Center" VerticalOptions="Start" >
                                <Grid.GestureRecognizers>
                                    <TapGestureRecognizer Command="{Binding SelectModeCommand}" CommandParameter="{Binding .}" />
                                </Grid.GestureRecognizers>
                                
                                <localButtons:CircleButton x:Name="checkbox"
                                                           InputTransparent="true"
                                                           IsCircle="True" IsTogglable="false"
                                                           Style="{StaticResource CheckboxButton}"
                                                           ImageNormal="icon_edit" ImagePressed="icon_edit"
                                                           VerticalOptions="Start" />
                            </Grid>
                            
                            <StackLayout Grid.Column="1" Orientation="Vertical" Spacing="6" HorizontalOptions="StartAndExpand" VerticalOptions="StartAndExpand" >
                                <StackLayout Orientation="Horizontal" Spacing="4" IsVisible="{Binding Title, Converter={StaticResource ObjectNotNullCheckConverter}}" HorizontalOptions="StartAndExpand" VerticalOptions="Center">
                                    <StackLayout.Padding>
                                        <OnPlatform x:TypeArguments="Thickness" iOS="0,4,0,0" Android="0"/>
                                    </StackLayout.Padding>
                                    
                                    <Label Text="{helpers:Translate New}" IsVisible="{Binding IsNew}" Style="{StaticResource Label_Label}" TextColor="{StaticResource Gold}" LineBreakMode="TailTruncation" VerticalTextAlignment="End"/>
                                    <Label Text="{Binding Title}" Style="{StaticResource H2_Label}" TextColor="{StaticResource LightBlue}" LineBreakMode="TailTruncation" VerticalTextAlignment="End"/>
                                    <Label Text="{helpers:Translate ByBabyationLower}" IsVisible="{Binding IsPredefined}" Style="{StaticResource Label_Label}" TextColor="{StaticResource MedPink}" LineBreakMode="TailTruncation" VerticalTextAlignment="End"/>
                                </StackLayout>
                                    
                                <Label MaxLines="2"
                                       Style="{StaticResource Label_Label}" 
                                       TextColor="{StaticResource LightBlue}" 
                                       VerticalTextAlignment="Start" 
                                       LineBreakMode="TailTruncation"
                                       HorizontalOptions="Fill" VerticalOptions="Start">
                                    <Label.FormattedText>
                                        <FormattedString>
                                            <Span Text="{Binding Description}" LineHeight="1.2" />
                                        </FormattedString>
                                    </Label.FormattedText>
                                </Label>
                            </StackLayout>
                        </Grid>
                    </Grid>
                </ViewCell>
            </DataTemplate>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <!-- Checkable Mode Cell -->
            <DataTemplate x:Key="ModeCellTemplate">
                <ViewCell>
                    <Grid Padding="2,3" MinimumHeightRequest="70" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" Margin="0,10,0,10">
