            <Style x:Key="Form_ElementEntry_Bold" BasedOn="{StaticResource Form_ElementEntry}" TargetType="localEditors:EntryEx">
                <Setter Property="FontFamily" Value="{StaticResource fontHurmeBold}" />
                <Setter Property="FontAttributes" Value="Bold" />
