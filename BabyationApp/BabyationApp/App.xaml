                    <Setter.Value>
                        <OnPlatform x:TypeArguments="x:Double" iOS="12" Android="14" />
                    </Setter.Value>
                </Setter>
            </Style>
            <Style x:Key="AuthenticationActionButton" TargetType="localButtons:ButtonEx">
                <Setter Property="BackgroundColorNormal" Value="{StaticResource Green}" />
