                <Setter Property="VerticalOptions" Value="Center" />
            </Style>
            <!-- END Navigation header style: label -->
            
            <Style x:Key="TappableLabel" BasedOn="{StaticResource BodyCopy_Label}" TargetType="localViews:LabelEx">
                <Setter Property="IsUnderlined" Value="true" />
                <Setter Property="TextColor" Value="{StaticResource MedBlue}" />
